@{
  Layout = "_Layout";
}
<div id='show'>
  <div id="showText">
      <h3>Pre order customers:</h3>

      @if (Model.Count != 0)
      {
          @foreach (var customer in Model)
          {
            <p><a href='/customer/@customer.GetId()'>@customer.GetFirstLast()</a></p>
          }
      }
      <button type="button" onClick="javascript:location.href ='/customer/new'">Add Customers</button>
  </div>
</div>
